{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.player.weapon_10.root": {
      "initial_state": "default",
      "states": {
        "default": {
          "on_entry": [ ""],
          "transitions": [
            { "attack": "v.attack_time" },
						{ "use": "q.is_using_item" }
					]
        },
        "attack": {
          "on_entry": [ "v.timer1 = q.life_time + 0.2;" ],
          "transitions": [
            { "attack2": "v.timer1 <= q.life_time" }
          ]
        },
        "attack2": {
          "on_entry": [ "/summon ks:trigger_dummy ^^^1.5 e:weapon_10.attack" ],
          "transitions": [
            { "default": "1.0" }
          ]
        },
        "use": {
          "on_entry": [ "v.timer2 = q.life_time + 0.2;" ],
          "transitions": [
            { "use2": "v.timer2 <= q.life_time" }
          ]
        },
        "use2": {
          "on_entry": [ "/effect @e[type=!player,family=!inanimate] levitation 1 5" ],
          "transitions": [
            { "default": "1.0" }
          ]
        }
      }
    }
  }
}
