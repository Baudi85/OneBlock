{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.effect.spider_cobweb" : {
			"initial_state" : "initial",
			"states" : {
				"initial": {
					"transitions": [
						{
							"particles": "(1.0)"
						}
					]
				},
				"particles": {
					"on_entry": [
						"/setblock ~ ~ ~ web",
						"/summon spider ~ ~+2 ~"
					],
					"transitions": [
						{
							"stage_0": "(1.0)"
						}
					]
				},
				"stage_0" : {
					"transitions": [
                        {
                            "stage_1": "(1.0)"
                        }
                    ],
					"on_entry": [
						"/setblock ~ ~ ~ web"
                    ]
				},
                "stage_1": {
                    "on_entry": [
						"/setblock ~ ~ ~ web"
                    ],
					"transitions": [
                        {
                            "stage_0": "(1.0)"
                        }
                    ]
                }
			}
		}
	}
}
